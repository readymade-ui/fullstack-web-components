  "scripts": {
    "analyze": "custom-elements-manifest analyze --dev --exclude \"[./**/*.stories.js, ./cypress/**/*.*, dist]\"",
    "clean": "lerna run clean --parallel",
    "build": "lerna run build",
    "start": "yarn storybook",
    "start:app": "lerna exec --scope @in/client -- yarn build && concurrently yarn:start:client yarn:start:server",
    "start:client": "lerna exec --scope @in/client -- yarn start",
    "start:server": "lerna exec --scope @in/server -- yarn dev",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "yarn build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "jest",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:snapshot": "npm test web-components-storyshots.test.js",
    "test:e2e:ci": "start-server-and-test storybook http://localhost:6006 test:e2e",
    "test:snapshot:ci": "start-server-and-test storybook http://localhost:6006 test:snapshot"
  },
  "private": true,
  "workspaces": [
    "packages/**/*"
  ]
